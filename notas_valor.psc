Algoritmo notas_valor
	DEFINIR NOTA Como Entero
	Escribir "este progrma debuelve elvalor segun la nota "
	escribir "DIGITE SU NOTA DE 1 A 5 QUE OBTUBO "
	LEER NOTA;
	Segun NOTA  Hacer
		1:
			Escribir  "DEFICIENTE ";
		2:
			Escribir "INSUFICIENTE";
		3:
			Escribir "ACEPTABLE";
		4:
			Escribir  "SUBRESALIENTE ";
		5:
			Escribir "EXELENTE";
			
		De Otro Modo:
			ESCRIBIR "EL VALOR INGRESADO ES INCORRECTO DIGITE UN NUMERO DE 1 A 5 ";
	Fin Segun
	
FinAlgoritmo
